else
  export NODE_PATH="/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/@antfu+eslint-config@4.16.2_@unocss+eslint-plugin@66.3.3_eslint@9.31.0_jiti@2.4.2__type_597344c4ccd492d695e8ceaebee8db0b/node_modules/@antfu/eslint-config/bin/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/@antfu+eslint-config@4.16.2_@unocss+eslint-plugin@66.3.3_eslint@9.31.0_jiti@2.4.2__type_597344c4ccd492d695e8ceaebee8db0b/node_modules/@antfu/eslint-config/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/@antfu+eslint-config@4.16.2_@unocss+eslint-plugin@66.3.3_eslint@9.31.0_jiti@2.4.2__type_597344c4ccd492d695e8ceaebee8db0b/node_modules/@antfu/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/@antfu+eslint-config@4.16.2_@unocss+eslint-plugin@66.3.3_eslint@9.31.0_jiti@2.4.2__type_597344c4ccd492d695e8ceaebee8db0b/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/node_modules:$NODE_PATH"
