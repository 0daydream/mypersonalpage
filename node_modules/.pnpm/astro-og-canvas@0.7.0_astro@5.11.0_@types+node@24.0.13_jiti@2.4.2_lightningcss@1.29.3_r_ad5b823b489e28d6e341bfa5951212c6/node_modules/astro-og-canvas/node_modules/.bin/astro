#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.29.3_rollup@4.45.0_terser@5._b24729c1d3b98ab3b7f8816d239a9f89/node_modules/astro/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.29.3_rollup@4.45.0_terser@5._b24729c1d3b98ab3b7f8816d239a9f89/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.29.3_rollup@4.45.0_terser@5._b24729c1d3b98ab3b7f8816d239a9f89/node_modules/astro/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/astro@5.11.0_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.29.3_rollup@4.45.0_terser@5._b24729c1d3b98ab3b7f8816d239a9f89/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../astro@5.11.0_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.29.3_rollup@4.45.0_terser@5._b24729c1d3b98ab3b7f8816d239a9f89/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../astro@5.11.0_@types+node@24.0.13_jiti@2.4.2_lightningcss@1.29.3_rollup@4.45.0_terser@5._b24729c1d3b98ab3b7f8816d239a9f89/node_modules/astro/astro.js" "$@"
fi
