#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/tsx@4.20.3/node_modules/tsx/dist/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/tsx@4.20.3/node_modules/tsx/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/tsx@4.20.3/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/tsx@4.20.3/node_modules/tsx/dist/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/tsx@4.20.3/node_modules/tsx/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/tsx@4.20.3/node_modules:/mnt/801C3A2F1C3A2118/Porn/Web Dev/JS/mypersonalpage/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../tsx@4.20.3/node_modules/tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../../../../../tsx@4.20.3/node_modules/tsx/dist/cli.mjs" "$@"
fi
